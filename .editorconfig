# http://editorconfig.org

root = true

[*]
indent_style = tab
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf
charset = utf-8

# Docstrings and comments use max_line_length = 79
[*.gd]
max_line_length = 119

[*.json]
indent_style = space
indent_size = 2
